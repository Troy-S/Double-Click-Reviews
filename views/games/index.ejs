<div class="games-index">

  <h2>Latest Games</h2>
  <a href="/games/new">Add a game</a>

  <div class="row">
    <ul>
      <% for (var i = games.length - 1; i >= 0; i--) { %>
      <li>
        <div class="col-lg-3">
          <ul>
            <li><a href="/games/<%= games[i].id %>"><img src="<%= games[i].image %>"></li></a>
            <li><%= games[i].title %></li>
            <li><%= games[i].platform %></li>
          </ul>
        </div>
      </li>
      <% }; %>
    </ul>
  </div>
</div>
