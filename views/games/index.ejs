<h3>Listing Games</h3>

<ul>
  <% for (var i = games.length - 1; i >= 0; i--) { %>
    <li>
      <ul>
        <li>
          <a href="/games/<%= games[i].id %>">
            <%= games[i].title %>
          </a>
        </li>
        <li>
          <a href="/games/<%= games[i].id %>/edit">
            Edit
          </a>
        </li>
      </ul>
    </li>
  <% }; %>
</ul>